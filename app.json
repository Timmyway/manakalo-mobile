{
  "expo": {
    "name": "Manakalo",
    "slug": "manakalo",
    "scheme": "manakalo",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.timmyway.manakalo",
      "buildNumber": "1.0.0",
      "icon": "./assets/icon.png"
    },
    "android": {
      "package": "com.timmyway.manakalo",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "0e768af8-5e35-48de-bce7-152a6b413f62"
      }
    }
  }
}
